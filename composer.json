{
    "name": "vangaurd/mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/vangaurdkarthik/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "mongo-passport"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5 || ^6.0 || ^8.0",
        "jenssegers/mongodb": "3.3.* || 3.4.* || 3.5.* || 3.6.* || ^3.8",
        "laravel/passport": "6.0.* || 7.0.* || 7.4.* || 7.5.* || ^8.0 || ^9.0 || ^10.1"
    },
    "autoload": {
        "psr-4": {
            "MongoPassport\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "ArkhaSystems",
            "email": "support@arkhasystems.com",
            "role": "Support"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MongoPassport\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
